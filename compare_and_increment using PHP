<?php
//Initialize array
$input_1 = array(1,2,3); 
$input_2 = array(3,2,1);
$a=0;
$b=0;
for ($i = 0; $i < count($input_1); $i++) {  
    if($input_1[$i]>$input_2[$i]){
        $a++;
    }
    elseif($input_1[$i]<$input_2[$i]){
        $b++;
    }
    else{
       continue; 
    }
}
echo $a." ";
echo $b;
?>
